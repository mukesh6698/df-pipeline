		{
			"name": "[concat(parameters('factoryName'), '/ADLSbinary_to_poolDB')]",
			"type": "Microsoft.DataFactory/factories/dataflows",
			"apiVersion": "2018-06-01",
			"properties": {
				"type": "MappingDataFlow",
				"typeProperties": {
					"sources": [
						{
							"dataset": {
								"referenceName": "binarycsvADLS",
								"type": "DatasetReference"
							},
							"name": "binarycsvADLS"
						}
					],
					"sinks": [
						{
							"dataset": {
								"referenceName": "AzureSynapseAnalyticsTable1",
								"type": "DatasetReference"
							},
							"name": "pooldbstream"
						}
					],
					"transformations": [
						{
							"name": "selectstream"
						},
						{
							"name": "derivedstream"
						}
					],
					"scriptLines": [
						"source(output(",
						"          Column_1 as string,",
						"          Column_2 as string,",
						"          Column_3 as string,",
						"          Column_4 as string,",
						"          Column_5 as string,",
						"          Column_6 as string,",
						"          Column_7 as string,",
						"          Column_8 as string,",
						"          Column_9 as string,",
						"          Column_10 as string",
						"     ),",
						"     allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     ignoreNoFilesFound: false) ~> binarycsvADLS",
						"binarycsvADLS select(mapColumn(",
						"          IP = Column_1,",
						"          Timeline = Column_4,",
						"          Request = Column_6,",
						"          Status = Column_7,",
						"          Bytes = Column_8",
						"     ),",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true) ~> selectstream",
						"selectstream derive(Timeline = substring(Timeline, 2, length(Timeline))) ~> derivedstream",
						"derivedstream sink(allowSchemaDrift: true,",
						"     validateSchema: false,",
						"     input(",
						"          IP as string,",
						"          Timeline as string,",
						"          Request as string,",
						"          Status as integer,",
						"          Bytes as integer",
						"     ),",
						"     deletable:false,",
						"     insertable:true,",
						"     updateable:false,",
						"     upsertable:false,",
						"     format: 'table',",
						"     staged: true,",
						"     allowCopyCommand: true,",
						"     skipDuplicateMapInputs: true,",
						"     skipDuplicateMapOutputs: true,",
						"     errorHandlingOption: 'stopOnFirstError',",
						"     mapColumn(",
						"          IP,",
						"          Timeline,",
						"          Request,",
						"          Status,",
						"          Bytes",
						"     )) ~> pooldbstream"
					]
				}
			},
			"dependsOn": []
		},
