{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf6698"
		},
		"appvm_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'appvm'"
		},
		"pool_db_customer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'pool_db_customer'"
		},
		"storage6698_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'storage6698'"
		},
		"synapsepooldb_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'synapsepooldb'"
		},
		"appvm_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "D:\\nginx-1.25.1\\logs"
		},
		"appvm_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "appvm"
		},
		"storage6698_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storage6698.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf6698_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"pool_db_customer_connectionString": {
						"value": "[parameters('pool_db_customer_connectionString')]"
					},
					"storage6698_accountKey": {
						"value": "[parameters('storage6698_accountKey')]"
					},
					"synapsepooldb_connectionString": {
						"value": "[parameters('synapsepooldb_connectionString')]"
					},
					"storage6698_properties_typeProperties_url": {
						"value": "[parameters('storage6698_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf6698_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"appvm_password": {
						"value": "[parameters('appvm_password')]"
					},
					"appvm_properties_typeProperties_host": {
						"value": "[parameters('appvm_properties_typeProperties_host')]"
					},
					"appvm_properties_typeProperties_userId": {
						"value": "[parameters('appvm_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf6698_ArmTemplate_0')]"
			]
		}
	]
}